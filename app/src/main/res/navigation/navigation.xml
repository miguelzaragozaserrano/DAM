<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/cameras_fragment">
    <fragment
        android:id="@+id/cameras_fragment"
        android:name="com.miguelzaragoza.upm.dam.ui.cameras.CamerasFragment"
        android:label="cameras_fragment"
        tools:layout="@layout/fragment_cameras">
        <action
            android:id="@+id/action_cameras_fragment_to_map_fragment"
            app:destination="@id/map_fragment" />
    </fragment>
    <fragment
        android:id="@+id/map_fragment"
        android:name="com.miguelzaragoza.upm.dam.ui.map.MapFragment"
        android:label="map_fragment"
        tools:layout="@layout/fragment_map"/>
</navigation>